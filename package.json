        "build": "gulp build",
        "build:examples": "gulp build-examples",
        "build:npm": "gulp build-npm",
        "publish:npm": "gulp publish-npm",
        "lint": "ng lint",
        "test": "ng test",
        "e2e": "ng e2e",
        "build:app": "ng build",
        "speak": "cd ./examples/my-speech-speak && npm install && npm start",
        "speak-offline": "cd ./examples/my-speech-speak-offline && npm install && npm start",
        "intent": "cd ./examples/my-speech-intent && npm install && npm start",
        "listen": "cd ./examples/my-speech-listen && npm install && npm start",
        "action": "cd ./examples/my-speech-action && npm install && npm start",
        "bot": "cd ./examples/my-speech-bot && npm install && npm start",
        "bot-offline": "cd ./examples/my-speech-bot-offline && npm install && npm start",
        "install:cordova": "gulp install-cordova",
        "install:cordova:ios": "gulp cordova-install-ios",
        "build:cordova:android": "ng build && gulp cordova-build-android",
        "cordova": "ng build && gulp cordova-browser",
        "cordova:android": "ng build&& gulp cordova-android",
        "cordova:ios": "ng build && gulp cordova-ios"
    },
    "dependencies": {
        "@angular/animations": "~12.1.1",
